{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api.py" }
  ],
  {
  "functions": {
    "api.py": {
      "memory": 3008,
      "maxDuration": 30
    },
    "/*.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
}
